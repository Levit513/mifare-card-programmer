{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/513programmer/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/513programmer",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
